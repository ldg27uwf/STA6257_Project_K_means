@article{crespo-facorro_aripiprazole_2021,
	title = {Aripiprazole as a Candidate Treatment of {COVID}-19 Identified Through Genomic Analysis},
	volume = {12},
	issn = {1663-9812},
	url = {https://www.frontiersin.org/journals/pharmacology/articles/10.3389/fphar.2021.646701/full},
	doi = {10.3389/fphar.2021.646701},
	abstract = {{\textless}p{\textgreater}{\textless}bold{\textgreater}Background:{\textless}/bold{\textgreater} Antipsychotics modulate expression of inflammatory cytokines and inducible inflammatory enzymes. Elopiprazole (a phenylpiperazine antipsychotic drug in phase 1) has been characterized as a therapeutic drug to treat {SARS}-{CoV}-2 infection in a repurposing study. We aim to investigate the potential effects of aripiprazole (an {FDA} approved phenylpiperazine) on {COVID}-19-related immunological parameters.{\textless}/p{\textgreater}{\textless}p{\textgreater}{\textless}bold{\textgreater}Methods:{\textless}/bold{\textgreater} Differential gene expression profiles of non-{COVID}-19 vs. {COVID}-19 {RNA}-Seq samples ({CRA}002390 project in {GSA} database) and drug-naïve patients with non-affective psychosis at baseline and after three months of aripiprazole treatment were identified. An integrative transcriptomic analyses of aripiprazole effects on differentially expressed genes in {COVID}-19 patients was performed.{\textless}/p{\textgreater}{\textless}p{\textgreater}{\textless}bold{\textgreater}Findings:{\textless}/bold{\textgreater} 82 out the 377 genes (21.7\%) with expression significantly altered by aripiprazole have also their expression altered in {COVID}-19 patients and in 93.9\% of these genes their expression is reverted by aripiprazole. The number of common genes with expression altered in both analyses is significantly higher than expected (Fisher’s Exact Test, two tail; {\textless}italic{\textgreater}p{\textless}/italic{\textgreater} value = 3.2e-11). 11 {KEGG} pathways were significantly enriched with genes with altered expression both in {COVID}-19 patients and aripiprazole medicated non-affective psychosis patients ({\textless}italic{\textgreater}p{\textless}/italic{\textgreater} adj\&lt;0.05). The most significant pathways were associated to immune responses and mechanisms of hyperinflammation-driven pathology (i.e.,“inflammatory bowel disease ({IBD})” (the most significant pathway with a {\textless}italic{\textgreater}p{\textless}/italic{\textgreater} adj of 0.00021), “Th1 and Th2 cell differentiation” and “B cell receptor signaling pathway”) that have been also associated with {COVID}19 clinical outcome.{\textless}/p{\textgreater}{\textless}p{\textgreater}{\textless}bold{\textgreater}Interpretation:{\textless}/bold{\textgreater} This exploratory investigation may provide further support to the notion that a protective effect is exerted by aripiprazole (phenylpiperazine) by modulating the expression of genes that have shown to be altered in {COVID}-19 patients. Along with many ongoing studies and clinical trials, repurposing available medications could be of use in countering {SARS}-{CoV}-2 infection, but require further studies and trials.{\textless}/p{\textgreater}},
	journaltitle = {Frontiers in Pharmacology},
	shortjournal = {Front. Pharmacol.},
	author = {Crespo-Facorro, Benedicto and Ruiz-Veguilla, Miguel and Vázquez-Bourgon, Javier and Sánchez-Hidalgo, Ana C. and Garrido-Torres, Nathalia and Cisneros, Jose M. and Prieto, Carlos and Sainz, Jesus},
	urldate = {2024-06-11},
	date = {2021-03-02},
	note = {Publisher: Frontiers},
	keywords = {psychosis, Inflammation, Coronavirus, elopiprazole, immunology, Repurposing drugs},
}

@article{suwannasom_detection_2019,
	title = {Detection of {CD}33 expression on monocyte surface is influenced by phagocytosis and temperature},
	volume = {38},
	issn = {0231-5882},
	doi = {10.4149/gpb_2019021},
	abstract = {{CD}33 is a myeloid-associated marker and belongs to the sialic acid-binding immunoglobulin (Ig)-like lectin (Siglec) family. Such types of receptors are highly expressed in acute myeloid leukemia, which could be used in its treatment. {CD}33 shows high variability in its expression levels with still unknown reasons. Here, we investigated the {CD}33 expression of monocytes in human blood samples processed at different temperatures and in dependence on their phagocytic activity against opsonized Escherichia coli. The samples were stained by fluorescently labelled anti-human {CD}14 to specify the monocyte population, anti-human {CD}33 antibodies to evaluate {CD}33 expression and analyzed by flow cytometry and confocal laser scanning microscopy. In blood samples kept at 37°C or first pre-chilled at 0°C with subsequent warming up to 37°C, the percentage of {CD}33-positive monocytes as well as their relative fluorescence intensity was up-regulated compared to samples kept constantly at 0°C. After exposure to E. coli the {CD}33 relative fluorescence intensity of the monocytes activated at 37°C was 3 to 4 times higher than that of those cells kept inactive at 0°C. Microscopic analysis showed internalisation of {CD}33 due to its enhanced expression on the surface followed by engulfment of E. coli.},
	pages = {369--378},
	number = {5},
	journaltitle = {General Physiology and Biophysics},
	shortjournal = {Gen Physiol Biophys},
	author = {Suwannasom, Nittiya and Smuda, Kathrin and Kloypan, Chiraphat and Kaewprayoon, Waraporn and Baisaeng, Nuttakorn and Boonla, Chanchai and Georgieva, Radostina and Bäumler, Hans},
	date = {2019-09},
	pmid = {31411573},
	note = {Number: 5},
	keywords = {Humans, Monocytes, Phagocytosis, Escherichia coli, Lipopolysaccharide Receptors, Sialic Acid Binding Ig-like Lectin 3, Temperature},
}

