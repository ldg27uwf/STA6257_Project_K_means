	author = {Greggio, Elisa and Jain, Shushant and Kingsbury, Ann and Bandopadhyay, Rina and Lewis, Patrick and Kaganovich, Alice and van der Brug, Marcel P. and Beilina, Alexandra and Blackinton, Jeff and Thomas, Kelly Jean and Ahmad, Rili and Miller, David W. and Kesavapany, Sashi and Singleton, Andrew and Lees, Andrew and Harvey, Robert J. and Harvey, Kirsten and Cookson, Mark R.},
	urldate = {2023-05-15},
	date = {2006-08-01},
	langid = {english},
	note = {Number: 2},
	keywords = {Parkinson's disease, Kinase, {LRRK}2, Substantia nigra, α-Synuclein},
}

