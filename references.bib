	pmid = {32746451},
	pmcid = {PMC7398703},
	file = {PubMed Central Full Text PDF:C\:\\Users\\linds\\Zotero\\storage\\UXQ8S5BD\\Badran et al. - 2020 - Antipsychotics drug aripiprazole as a lead against.pdf:application/pdf},
