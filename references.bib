	number = {2},
	journaltitle = {Disease Models \& Mechanisms},
	shortjournal = {Dis Model Mech},
	author = {Besnard, Marine and Padonou, Francine and Provin, Nathan and Giraud, Matthieu and Guillonneau, Carole},
