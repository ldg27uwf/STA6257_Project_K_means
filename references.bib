	number = {1},
	journaltitle = {Neoplasia (New York, N.Y.)},
	shortjournal = {Neoplasia},
	author = {Kline, Christina Leah B. and Ralff, Marie D. and Lulla, Amriti R. and Wagner, Jessica M. and Abbosh, Phillip H. and Dicker, David T. and Allen, Joshua E. and El-Deiry, Wafik S.},
