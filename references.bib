@article{filippi_mechanism_2016,
	title = {Mechanism of Diapedesis: Importance of the Transcellular Route},
	volume = {129},
	issn = {0065-2776},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4889131/},
	doi = {10.1016/bs.ai.2015.09.001},
	shorttitle = {Mechanism of Diapedesis},
	abstract = {The neutrophil transmigration across the blood endothelial cell barrier represents the prerequisite step of innate inflammation. Neutrophil recruitment to inflamed tissues occurs in a well-defined stepwise manner, which includes elements of neutrophil rolling, firm adhesion, and crawling onto the endothelial cell surface before trans-migrating across the endothelial barrier. This latter step known as diapedesis can occur at the endothelial cell junction (paracellular) or directly through the endothelial cell body (transcellular). The extravasation cascade is controlled by series of engagement of various adhesive modules, which result in activation of bidirectional signals to neutrophils and endothelial cells for adequate cellular response. This review will focus on recent advances in our understanding of mechanism of leukocyte crawling and diapedesis, with an emphasis on leukocyte–endothelial interactions and the signaling pathways they transduce to determine the mode of diapedesis, junctional or non-junctional. I will also discuss emerging evidence highlighting key differences in the two modes of diapedesis and why it is clinically important to understand specificity in the regulation of diapedesis.},
	pages = {25--53},
	journaltitle = {Advances in immunology},
	shortjournal = {Adv Immunol},
	author = {Filippi, Marie-Dominique},
	urldate = {2023-04-25},
	date = {2016},
	pmid = {26791857},
	pmcid = {PMC4889131},
	file = {PubMed Central Full Text PDF:C\:\\Users\\linds\\Zotero\\storage\\25U5QXMW\\Filippi - 2016 - Mechanism of Diapedesis Importance of the Transce.pdf:application/pdf},
}

@incollection{tahir_neutrophilia_2023,
