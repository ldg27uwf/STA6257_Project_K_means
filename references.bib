	number = {2},
	journaltitle = {Journal of Population Therapeutics and Clinical Pharmacology = Journal De La Therapeutique Des Populations Et De La Pharmacologie Clinique},
	shortjournal = {J Popul Ther Clin Pharmacol},
	author = {Jabeen, Shaik Afshan and Gaddamanugu, Padmaja and Cherian, Ajith and Mridula, Kandadai Mridula and Kumar, Dasari Uday and Meena, Angamuttu Kanikannan},
	date = {2018-07-11},
	pmid = {30725538},
	keywords = {Adult, Humans, Middle Aged, Female, Male, Magnetic Resonance Imaging, Child, Follow-Up Studies, Risk Factors, Electroencephalography, Epilepsy, Seizures, Young Adult, Adolescent, Anticonvulsants, Anti-Anxiety Agents, Anxiety, Depression, Levetiracetam, Prospective Studies, Psychotherapy},
